# Database Configuration
DATABASE_URL=postgresql://user:password@host:port/database

# For Railway (get from Railway dashboard)
# DATABASE_URL=postgresql://postgres:xxx@containers-us-west-xxx.railway.app:5432/railway

# Application Settings
DATA_DIR=/app/data
LOG_LEVEL=INFO

# AWS S3 (for downloading CSVs)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-west-2

# Railway Environment (set automatically by Railway)
RAILWAY_ENVIRONMENT=development
PORT=8000

# CORS Origins (for Vercel frontend)
ALLOWED_ORIGINS=http://localhost:3000,https://your-app.vercel.app
